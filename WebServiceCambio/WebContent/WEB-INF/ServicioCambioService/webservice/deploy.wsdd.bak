<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ServicioCambioService WSDL service -->

  <service name="ServicioCambio" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webservice"/>
      <parameter name="wsdlServiceElement" value="ServicioCambioService"/>
      <parameter name="schemaQualified" value="http://webservice,http://domain"/>
      <parameter name="wsdlServicePort" value="ServicioCambio"/>
      <parameter name="className" value="webservice.ServicioCambioSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ServicioCambio"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="guardarConsulta" qname="operNS:guardarConsulta" xmlns:operNS="http://webservice" returnQName="retNS:guardarConsultaReturn" xmlns:retNS="http://webservice" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:consulta" xmlns:pns="http://webservice" type="tns:ConsultaVO" xmlns:tns="http://domain"/>
      </operation>
      <parameter name="allowedMethods" value="guardarConsulta"/>

      <typeMapping
        xmlns:ns="http://domain"
        qname="ns:ConsultaVO"
        type="java:domain.ConsultaVO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://domain"
        qname="ns:UsuarioVO"
        type="java:domain.UsuarioVO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://domain"
        qname="ns:TipoConsultaVO"
        type="java:domain.TipoConsultaVO"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
